{
  "name": "@gtm/app",
  "version": "0.0.1",
  "description": "Git Time Metric Web Dashboard - Web App",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "browserslist": [
    "last 1 chrome versions"
  ],
  "scripts": {
    "build:assets": "mkdir -p dist/assets && cp -v -r assets dist/",
    "build:css": "NODE_ENV=production postcss main.pcss -o dist/assets/main.css",
    "dev": "parcel serve dev/index.html",
    "build:electron": "rollup --config",
    "build": "cp -v -r assets dist/ && parcel build index.ts --out-dir dist --public-url ./ --target=node",
    "build4": "cp -v -r assets dist/ && parcel build index.html --out-dir dist --public-url ./ --no-source-maps",
    "serve": "cp -v -r src/assets dist/ && parcel serve index.html --no-source-maps --no-hmr",
    "build2": "parcel build index.html --no-source-maps --public-url /gtm-web/ && cp -v data-commits.json dist/",
    "test": "mocha dist/test/*-test.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "chart.js": "^2.9.3",
    "chartjs-chart-matrix": "^0.1.3",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "moment": "^2.24.0",
    "page": "^1.11.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/chart.js": "^2.9.19",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.1",
    "@types/nodegit": "^0.26.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "autoprefixer": "^9.7.6",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.3.5",
    "rollup": "^2.7.3",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-copy-assets": "^2.0.1",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.1",
    "sass": "^1.26.5",
    "svelte": "^3.19.0",
    "svelte-preprocess": "^3.7.4",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "license": "CC0-1.0"
}
