{
  "name": "gtmweb",
  "version": "0.0.1",
  "description": "Git Time Metric Web Console",
  "keywords": [
    "git",
    "gtm",
    "metrics",
    "time-tracker",
    "productivity",
    "dashboard"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 1 chrome versions"
  ],
  "scripts": {
    "dev": "cp -v -r src/assets dist && parcel watch index.html test/*.html test/*-test.ts --no-source-maps --no-cache --no-hmr",
    "build2": "parcel build index.html --no-source-maps --public-url /gtm-web/ && cp -v data-commits.json dist/",
    "build": "parcel build index.html --no-source-maps && cp -v data-commits.json dist/",
    "watch": "tsc --watch",
    "test": "mocha dist/test/*-test.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@popperjs/core": "^2.3.3",
    "bootstrap": "^4.4.1",
    "bootstrap-select": "^1.13.15",
    "chart.js": "^2.9.3",
    "chartjs-chart-matrix": "^0.1.3",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-zoom": "^0.7.5",
    "daterangepicker": "^3.0.5",
    "jquery": "^3.5.0",
    "moment": "^2.24.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.2",
    "@types/chart.js": "^2.9.19",
    "@types/daterangepicker": "^3.0.1",
    "@types/jquery": "^3.3.35",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "sass": "^1.26.5",
    "svelte": "^3.21.0",
    "typescript": "^3.8.3"
  },
  "author": "Luis Mastrangelo",
  "license": "ISC"
}
