{
  "name": "gtmweb",
  "version": "0.0.1",
  "description": "Git Time Metric Web Console",
  "keywords": [
    "git",
    "gtm",
    "metrics",
    "time-tracker",
    "productivity",
    "dashboard"
  ],
  "main": "dist/index.html",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev4": "parcel build index.html",
    "dev": "rollup --config --watch",
    "dev2": "parcel serve index.html",
    "serve": "cp -v -r src/assets dist/ && parcel serve index.html --no-source-maps --no-hmr",
    "build2": "parcel build index.html --no-source-maps --public-url /gtm-web/ && cp -v data-commits.json dist/",
    "build": "parcel build index.html --no-source-maps && cp -v data-commits.json dist/",
    "watch": "tsc --watch",
    "test": "mocha dist/test/*-test.js"
  },
  "dependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "chart.js": "^2.9.3",
    "chartjs-chart-matrix": "^0.1.3",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "moment": "^2.24.0",
    "rollup-plugin-bundle-html": "^0.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/bootstrap": "^4.3.2",
    "@types/chart.js": "^2.9.19",
    "@types/daterangepicker": "^3.0.1",
    "@types/jquery": "^3.3.35",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "parcel": "^2.0.0-alpha.3.2",
    "parcel-transformer-svelte": "^1.0.3",
    "parcel-transformer-svelte3": "^1.0.0-alpha.1.7",
    "rollup": "^2.7.3",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.1",
    "sass": "^1.26.5",
    "svelte": "^3.19.0",
    "svelte-preprocess": "^3.7.4",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "author": "Luis Mastrangelo",
  "license": "ISC"
}
